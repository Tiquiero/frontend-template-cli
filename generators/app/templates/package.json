{
  "name": "ant-design-pro",
  "version": "4.0.0",
  "private": true,
  "description": "An out-of-box UI solution for enterprise applications",
  "scripts": {
    "analyze": "cross-env ANALYZE=1 umi build",
    "build": "umi build",
    "deploy": "npm run site && npm run gh-pages",
    "dev": "npm run start:dev",
    "fetch:blocks": "pro fetch-blocks --branch=umi@3 && npm run prettier",
    "gh-pages": "cp CNAME ./dist/ && gh-pages -d dist",
    "i18n-remove": "pro i18n-remove --locale=zh-CN --write",
    "postinstall": "umi g tmp",
    "lint": "umi g tmp && npm run lint:js && npm run lint:style && npm run lint:prettier",
    "lint-staged": "lint-staged",
    "lint-staged:js": "eslint --ext .js,.jsx,.ts,.tsx ",
    "lint:fix": "eslint --fix --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src && npm run lint:style",
    "lint:js": "eslint --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src",
    "lint:prettier": "prettier --check \"**/*\" --end-of-line auto",
    "lint:style": "stylelint --fix \"src/**/*.less\" --syntax less",
    "prettier": "prettier -c --write \"**/*\"",
    "start": "umi dev",
    "start:dev": "cross-env REACT_APP_ENV=dev MOCK=none umi dev",
    "start:no-mock": "cross-env MOCK=none umi dev",
    "start:no-ui": "cross-env UMI_UI=none umi dev",
    "start:pre": "cross-env REACT_APP_ENV=pre umi dev",
    "start:test": "cross-env REACT_APP_ENV=test MOCK=none umi dev",
    "pretest": "node ./tests/beforeTest",
    "test": "umi test",
    "test:all": "node ./tests/run-tests.js",
    "test:component": "umi test ./src/components",
    "tsc": "tsc",
    "pre-commit": "lint-staged"
  },
  "husky": {
    "hooks": {}
  },
  "lint-staged": {
    "**/*.less": "stylelint --syntax less",
    "**/*.{js,jsx,ts,tsx}": "npm run lint-staged:js",
    "**/*.{js,jsx,tsx,ts,less,md,json}": [
      "prettier --write"
    ]
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 10"
  ],
  "dependencies": {
    "@ant-design/icons": "^4.6.2",
    "@ant-design/pro-form": "1.31.2",
    "@ant-design/pro-layout": "^6.21.1",
    "@ant-design/pro-table": "^2.43.4",
    "@antv/data-set": "^0.11.8",
    "@antv/g6": "^4.3.4",
    "@antv/l7": "^2.4.4",
    "@antv/l7-maps": "^2.4.4",
    "@apulis/authz": "^1.6.14",
    "@apulis/constants": "^0.3.9",
    "@apulis/first-login": "^1.3.0",
    "@apulis/provider": "^0.6.0",
    "@apulis/table": "^1.8.13",
    "@types/lodash.debounce": "^4.0.6",
    "@types/lodash.isequal": "^4.5.5",
    "@types/uuid": "^8.3.1",
    "@typescript-eslint/eslint-plugin": "^4.28.3",
    "antd": "^4.16.7",
    "axios": "^0.21.1",
    "bizcharts": "^4.1.11",
    "bizcharts-plugin-slider": "^3.0.1",
    "child_process": "^1.0.2",
    "classnames": "^2.3.1",
    "connect-history-api-fallback": "^1.6.0",
    "crypto-js": "^4.0.0",
    "ecstatic": "^4.1.4",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-jest": "^24.3.6",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-unicorn": "^34.0.1",
    "gg-editor": "^3.1.3",
    "insert-css": "^2.0.0",
    "lodash": "^4.17.21",
    "lodash-decorators": "^6.0.0",
    "lodash.debounce": "^4.0.8",
    "lodash.isequal": "^4.5.0",
    "mockjs": "^1.0.1-beta3",
    "moment": "^2.29.1",
    "numeral": "^2.0.6",
    "nzh": "^1.0.3",
    "omit.js": "^2.0.2",
    "path-to-regexp": "6.2.0",
    "prop-types": "^15.5.10",
    "qs": "^6.10.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-fittext": "^1.0.0",
    "react-helmet-async": "^1.0.9",
    "react-iframe": "^1.8.0",
    "react-router": "^5.2.0",
    "socket.io-client": "^4.1.3",
    "typescript": "^4.3.5",
    "umi": "^3.5.13",
    "umi-request": "^1.3.9",
    "use-merge-value": "^1.0.1"
  },
  "devDependencies": {
    "@ant-design/pro-cli": "^2.1.4",
    "@types/classnames": "^2.3.1",
    "@types/express": "^4.17.13",
    "@types/history": "^4.7.9",
    "@types/jest": "^26.0.24",
    "@types/lodash": "^4.14.171",
    "@types/qs": "^6.9.7",
    "@types/react": "^17.0.14",
    "@types/react-dom": "^17.0.9",
    "@types/react-helmet": "^6.1.2",
    "@typescript-eslint/parser": "^4.28.3",
    "@umijs/fabric": "^2.6.2",
    "@umijs/plugin-blocks": "^2.2.2",
    "@umijs/preset-ant-design-pro": "^1.3.3",
    "@umijs/preset-react": "^1.8.19",
    "@umijs/preset-ui": "^2.2.9",
    "carlo": "^0.9.46",
    "chalk": "^4.1.1",
    "cross-env": "^7.0.3",
    "cross-port-killer": "^1.3.0",
    "detect-installer": "^1.0.1",
    "enzyme": "^3.11.0",
    "eslint": "^7.31.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "express": "^4.17.1",
    "gh-pages": "^3.2.3",
    "husky": "^7.0.1",
    "jsdom-global": "^3.0.2",
    "lint-staged": "^11.0.1",
    "mockjs": "^1.0.1-beta3",
    "prettier": "^2.3.2",
    "pro-download": "1.0.1",
    "puppeteer-core": "^10.1.0",
    "stylelint": "^13.13.1"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "checkFiles": [
    "src/**/*.js*",
    "src/**/*.ts*",
    "src/**/*.less",
    "config/**/*.js*",
    "scripts/**/*.js"
  ]
}
